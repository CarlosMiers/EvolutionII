	<queryString>
		<![CDATA[SELECT cabecera_descuento_documentos.numero,clientes.codigo AS codcliente,clientes.ruc AS clienteruc,clientes.nombre AS clientenom,clientes.direccion AS clientedirec,
YEAR(cabecera_descuento_documentos.fecha) AS periodo,
CASE MONTH(cabecera_descuento_documentos.fecha)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes,
(SELECT MAX(DAY(vencimiento))
FROM detalle_descuento_documentos
WHERE detalle_descuento_documentos.ndescuento=cabecera_descuento_documentos.numero AND detalle_descuento_documentos.plazo) diavence,
(SELECT MAX(MONTH(vencimiento)) FROM detalle_descuento_documentos WHERE detalle_descuento_documentos.ndescuento=cabecera_descuento_documentos.numero AND detalle_descuento_documentos.plazo) mesvence,
(SELECT MAX(YEAR(vencimiento)) FROM detalle_descuento_documentos WHERE detalle_descuento_documentos.ndescuento=cabecera_descuento_documentos.numero AND detalle_descuento_documentos.plazo) anualvence,
(SELECT clientes.nombre FROM clientes WHERE clientes.codigo=cabecera_descuento_documentos.codcodeudor) codenombre,
(SELECT clientes.ruc FROM clientes WHERE clientes.codigo=cabecera_descuento_documentos.codcodeudor) coderuc,
(SELECT clientes.direccion FROM clientes WHERE clientes.codigo=cabecera_descuento_documentos.codcodeudor) codedireccion
FROM cabecera_descuento_documentos
LEFT JOIN detalle_descuento_documentos
ON detalle_descuento_documentos.ndescuento=cabecera_descuento_documentos.idprestamos
INNER JOIN clientes
ON clientes.codigo=cabecera_descuento_documentos.socio
WHERE cabecera_descuento_documentos.numero=$P{cNumero}]]>
	</queryString>
