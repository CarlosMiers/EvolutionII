	<queryString>
		<![CDATA[SELECT  cabecera_descuento_documentos.numero AS numero,
     cabecera_descuento_documentos.fecha AS fecha,
     cabecera_descuento_documentos.moneda AS moneda,
     cabecera_descuento_documentos.`sucursal` AS sucursal,
     cabecera_descuento_documentos.`socio` AS socio,
     cabecera_descuento_documentos.`comision_deudor` AS comision_deudor,
     cabecera_descuento_documentos.`deducciones` AS deducciones,
     cabecera_descuento_documentos.`importe` AS importe,
     cabecera_descuento_documentos.`montoiva` AS montoiva,
     cabecera_descuento_documentos.`totaldescuento` AS totaldescuento,
     cabecera_descuento_documentos.`oficialcta` AS oficialcta,
     cabecera_descuento_documentos.`observacion` AS observacion,
     detalle_descuento_documentos.`ndescuento` AS ndescuento,
     detalle_descuento_documentos.`emision` AS emision,
     detalle_descuento_documentos.`nrodocumento` AS nrodocumento,
     detalle_descuento_documentos.`comprobante` AS comprobante,
     detalle_descuento_documentos.`cargobanco` AS cargobanco,
     detalle_descuento_documentos.`monto_documento` AS monto_documento,
     detalle_descuento_documentos.`librador` AS librador,
     detalle_descuento_documentos.`vencimiento` AS vencimiento,
     detalle_descuento_documentos.`tasa` AS tasa,
     detalle_descuento_documentos.`plazo` AS plazo,
     detalle_descuento_documentos.`compra` AS compra,
     detalle_descuento_documentos.`descuento` AS descuento,
     detalle_descuento_documentos.`valor_actual` AS actual,
     detalle_descuento_documentos.`acreedor` AS acreedor,
     detalle_descuento_documentos.`nrocuentacte` AS nrocuentacte,
     bancos_plaza.`nombre` AS nombrebanco,
     vendedores.nombre as nombreoficial,
     clientes.nombre as nombrecliente
FROM
     cabecera_descuento_documentos
 INNER JOIN  detalle_descuento_documentos
 ON cabecera_descuento_documentos.`numero` = detalle_descuento_documentos.`ndescuento`
 INNER JOIN bancos_plaza
 ON detalle_descuento_documentos.`cargobanco` = bancos_plaza.`codigo`
INNER JOIN vendedores
ON cabecera_descuento_documentos.asesor=vendedores.codigo
INNER JOIN clientes
ON cabecera_descuento_documentos.socio=clientes.codigo
where cabecera_descuento_documentos.numero=$P{cNumero}]]>
	</queryString>
