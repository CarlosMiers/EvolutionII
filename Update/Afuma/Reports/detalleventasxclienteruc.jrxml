	<queryString>
		<![CDATA[select cliente,nombrecliente,
sum(monto) as total
from vista_detalle_ventas
 WHERE vista_detalle_ventas.fecha
 BETWEEN $P{FechaInicio} AND $P{FechaFinal}
 AND vista_detalle_ventas.ruc=$P{cCliente}
