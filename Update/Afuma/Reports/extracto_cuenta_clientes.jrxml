	<parameter name="cNombreEmpresa" class="java.lang.String"/>
	<parameter name="nCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT prestamos.numero,prestamos.fecha,socio,clientes.nombre AS nombrecliente,clientes.celular,clientes.direccion,clientes.mail,
MAX(cuenta_clientes.vencimiento) AS ultimovence,
comprobantes.nomalias AS nombreprestamo,prestamos.importe,cuenta_clientes.numerocuota,
prestamos.tasa,prestamos.interes,cuenta_clientes.importe AS importecuota,
prestamos.ivainteres,prestamos.totalamortizacion,prestamos.totalprestamo,monedas.etiqueta,
SUM(cuenta_clientes.saldo) AS saldoapagar,vendedores.nombre AS nombrevendedor,
(SELECT DATEDIFF(CURDATE(),cuenta_clientes.vencimiento)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = prestamos.idprestamos
AND cuenta_clientes.saldo>0 GROUP BY creferencia) atraso,
(SELECT COUNT(cuota)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = prestamos.idprestamos
AND cuenta_clientes.saldo=0 GROUP BY idprestamos) cuotapagada,
(SELECT MAX(fecha_pago)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = prestamos.idprestamos
GROUP BY creferencia) ultimopago
FROM prestamos
INNER JOIN cuenta_clientes
ON idprestamos=creferencia
INNER JOIN comprobantes
ON comprobantes.codigo=prestamos.tipo
INNER JOIN clientes
ON clientes.codigo=prestamos.socio
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
INNER JOIN vendedores
ON vendedores.codigo=clientes.asesor
WHERE socio=$P{nCliente}
GROUP BY creferencia
UNION
SELECT detalle_descuento_documentos.nrodocumento AS numero,cabecera_descuento_documentos.fecha,cabecera_descuento_documentos.socio,
clientes.nombre AS nombrecliente,
clientes.celular,clientes.direccion,clientes.mail,
MAX(cuenta_clientes.vencimiento) AS ultimovence,
comprobantes.nomalias AS nombreprestamo,detalle_descuento_documentos.monto_documento AS importe,cuenta_clientes.numerocuota,
detalle_descuento_documentos.tasa,00000000000.00  AS interes,cuenta_clientes.importe AS importecuota,
00000000000.00 AS ivainteres,detalle_descuento_documentos.monto_documento AS totalamortizacion,
detalle_descuento_documentos.monto_documento,monedas.etiqueta,
SUM(cuenta_clientes.saldo) AS saldoapagar,vendedores.nombre AS nombrevendedor,
(SELECT DATEDIFF(CURDATE(),cuenta_clientes.vencimiento)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = cabecera_descuento_documentos.idprestamos
AND cuenta_clientes.saldo>0 GROUP BY creferencia) atraso,
(SELECT MAX(cuota)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = cabecera_descuento_documentos.idprestamos
AND cuenta_clientes.saldo=0 GROUP BY creferencia) cuotapagada,
(SELECT MAX(fecha_pago)
FROM cuenta_clientes
WHERE cuenta_clientes.creferencia = cabecera_descuento_documentos.idprestamos
GROUP BY creferencia) ultimopago
FROM cabecera_descuento_documentos
INNER JOIN detalle_descuento_documentos
ON detalle_descuento_documentos.ndescuento=cabecera_descuento_documentos.numero
INNER JOIN cuenta_clientes
ON detalle_descuento_documentos.nrodocumento =cuenta_clientes.documento
INNER JOIN comprobantes
ON comprobantes.codigo=detalle_descuento_documentos.comprobante
INNER JOIN clientes
ON clientes.codigo=cabecera_descuento_documentos.socio
INNER JOIN vendedores
ON clientes.asesor=vendedores.codigo
INNER JOIN monedas
ON monedas.codigo=cabecera_descuento_documentos.moneda
WHERE socio=$P{nCliente}
GROUP BY cuenta_clientes.iddocumento]]>
	</queryString>
