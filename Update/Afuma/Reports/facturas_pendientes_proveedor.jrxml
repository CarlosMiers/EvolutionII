	<queryString>
		<![CDATA[SELECT saldo_proveedores.nrofactura,saldo_proveedores.proveedor,saldo_proveedores.emision,saldo_proveedores.vencimiento,saldo_proveedores.fecha_pago, DATEDIFF(saldo_proveedores.vencimiento, saldo_proveedores.emision) AS mora,
saldo_proveedores.cuota,saldo_proveedores.importe,saldo_proveedores.saldo,saldo_proveedores.nombre,proveedores.nombre  AS nombrepro,
proveedores.codigo AS cuentapro,proveedores.ruc AS rucpro,proveedores.direccion AS direccionpro,proveedores.direccion AS direccionpro, proveedores.telefono AS telefonopro
FROM saldo_proveedores
LEFT JOIN proveedores
ON proveedores.codigo=saldo_proveedores.proveedor
LEFT JOIN sucursales
ON sucursales.codigo=saldo_proveedores.sucursal
WHERE saldo_proveedores.saldo<>0
AND IF($P{nSucursal}<>0,saldo_proveedores.sucursal=$P{nSucursal},TRUE)
AND IF($P{nProveedor}<>0,saldo_proveedores.proveedor=$P{nProveedor},TRUE)
ORDER BY saldo_proveedores.proveedor,saldo_proveedores.vencimiento]]>
	</queryString>
