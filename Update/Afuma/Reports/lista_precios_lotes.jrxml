	<queryString>
		<![CDATA[SELECT
	lista_precio_lotes.idlista,
	lotes.codigo as idlote,
    lista_precio_lotes.descripcion as nombre,
	lotes.nombre as lote,
    loteamientos.nombre as loteamiento,
    manzanas.codigo as idmanzana,
    manzanas.nombre as manzana,
	lista_precio_lotes.plazo,
	lista_precio_lotes.cuotas,
	lista_precio_lotes.precio
FROM lista_precio_lotes
INNER JOIN lotes on lotes.codigo=lista_precio_lotes.lote
INNER JOIN manzanas on manzanas.codigo=lotes.manzana
INNER JOIN loteamientos on loteamientos.codigo=lotes.loteamiento
ORDER BY lista_precio_lotes.idlista;]]>
