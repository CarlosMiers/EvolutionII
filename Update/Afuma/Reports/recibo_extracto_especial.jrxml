	<queryString>
		<![CDATA[SELECT cierre_cobranzas.fecha,cierre_cobranzas.vencimiento,cierre_cobranzas.documento,cierre_cobranzas.comercial,
cierre_cobranzas.cliente,cierre_cobranzas.cobrado,cierre_cobranzas.enviado,cierre_cobranzas.cobrado,
clientes.nombre AS nombresocio,clientes.telefono,clientes.ruc,
cierre_cobranzas.cuota,cierre_cobranzas.numerocuota,
cierre_cobranzas.amortiza,cierre_cobranzas.minteres,
giradurias.nombre AS nombregiraduria,
comprobantes.nombre AS nombreservicio,
casas.nombre AS nombrecomercio,YEAR(cierre_cobranzas.fecha_cobro) AS periodo,
CASE MONTH(cierre_cobranzas.fecha_cobro)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes
FROM cierre_cobranzas
INNER JOIN clientes
ON clientes.codigo=cierre_cobranzas.cliente
LEFT JOIN giradurias
ON giradurias.codigo=clientes.giraduria
LEFT JOIN comprobantes
ON comprobantes.codigo=cierre_cobranzas.comprobante
LEFT JOIN casas
ON casas.codigo=cierre_cobranzas.comercial
WHERE cierre_cobranzas.cliente=$P{cCliente}
AND cierre_cobranzas.fecha_cobro between $P{FechaInicio} and $P{FechaFinal}
ORDER BY cierre_cobranzas.cliente]]>
	</queryString>
