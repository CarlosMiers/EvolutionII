	<queryString>
		<![CDATA[SELECT numero,cliente,SUM(amortiza) AS totalpago,clientes.nombre,clientes.ruc,cobranzas.fecha,detalle_cobranzas.vence
FROM cobranzas
INNER JOIN detalle_cobranzas
ON cobranzas.idpagos=detalle_cobranzas.iddetalle
INNER JOIN clientes
ON clientes.codigo=cobranzas.cliente
WHERE idpagos=$P{cReferencia}
GROUP BY idpagos]]>
	</queryString>
