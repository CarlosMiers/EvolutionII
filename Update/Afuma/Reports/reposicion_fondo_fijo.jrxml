	<queryString>
		<![CDATA[SELECT reposicion_fondos.numero,reposicion_fondos.fecha,
reposicion_fondos.sucursal,reposicion_fondos.asiento,
reposicion_fondos.banco,reposicion_fondos.importe,
reposicion_fondos.nrocheque,reposicion_fondos.cotizacion,
gastos_compras.ordentrabajo,gastos_compras.proveedor,gastos_compras.nrofactura,
gastos_compras.exentas,gastos_compras.gravadas10,gastos_compras.comprobante,
gastos_compras.gravadas5,gastos_compras.iva10,gastos_compras.iva5,
gastos_compras.totalneto,gastos_compras.idcta,
gastos_compras.fecha AS fechaproceso,proveedores.nombre AS nombreproveedor,
bancos.nombre as nombrebanco,comprobantes.nombre as nombrecomprobante,
plan.nombre as nombrecuenta
FROM reposicion_fondos
INNER JOIN gastos_compras
ON reposicion_fondos.numero=gastos_compras.fondofijo
INNER JOIN comprobantes
ON comprobantes.codigo=gastos_compras.comprobante
INNER JOIN proveedores
ON proveedores.codigo=gastos_compras.proveedor
INNER JOIN bancos
ON bancos.codigo=reposicion_fondos.banco
LEFT JOIN rubro_compras
ON rubro_compras.codigo=gastos_compras.concepto
LEFT JOIN plan
ON plan.codigo=rubro_compras.idcta
WHERE numero=$P{nNumero}]]>
