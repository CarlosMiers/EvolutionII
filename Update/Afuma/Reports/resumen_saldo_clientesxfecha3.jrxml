	<parameter name="cCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT productos.codigo,productos.nombre,SUM(cantidad) AS unidades,costo,
rubros.nombre AS nombrerubro
FROM productos
INNER JOIN rubros
ON rubros.codigo=productos.rubro
INNER JOIN detalle_ventas
ON codprod=productos.codigo
INNER JOIN cabecera_ventas
ON creferencia=dreferencia
WHERE cliente=$P{cCliente}
 GROUP BY productos.codigo ORDER BY unidades DESC
 LIMIT 10]]>
	</queryString>
