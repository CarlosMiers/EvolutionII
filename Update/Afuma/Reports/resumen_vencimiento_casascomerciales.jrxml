	<queryString>
		<![CDATA[SELECT cuenta_clientes.documento,casas.nombre AS nombrecomercio,cuenta_clientes.fecha,cuenta_clientes.vencimiento,
cuenta_clientes.numerocuota,cuenta_clientes.cuota,cuenta_clientes.comprobante,
cuenta_clientes.comercial,SUM(cuenta_clientes.importe) AS totaldeuda,comprobantes.nombre AS nombrecomprobante,cuenta_clientes.cliente,clientes.nombre as nombrecliente
FROM cuenta_clientes
LEFT JOIN casas
ON casas.codigo=cuenta_clientes.comercial
LEFT JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
LEFT JOIN clientes
ON clientes.codigo=cuenta_clientes.cliente
WHERE cuenta_clientes.comprobante=$P{cServicio}
AND cuenta_clientes.vencimiento
BETWEEN $P{FechaI} AND $P{FechaF}
GROUP BY cuenta_clientes.comercial
ORDER BY casas.codigo]]>
	</queryString>
