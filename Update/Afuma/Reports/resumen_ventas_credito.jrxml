	<queryString>
		<![CDATA[SELECT  cabecera_ventas.factura,cabecera_ventas.fecha,cabecera_ventas.fecha,cabecera_ventas.sucursal,
sucursales.nombre AS nombresucursal,clientes.codigo AS cuenta, clientes.nombre AS nombrecliente,
cabecera_ventas.totalneto
FROM cabecera_ventas
INNER JOIN sucursales
ON sucursales.codigo=cabecera_ventas.sucursal
INNER JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
INNER JOIN monedas
ON monedas.codigo=cabecera_ventas.moneda
WHERE cabecera_ventas.fecha BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
AND IF($P{nSucursal}<>0,
cabecera_ventas.sucursal=$P{nSucursal},TRUE)
AND cabecera_ventas.moneda=$P{nMoneda}
AND cabecera_ventas.comprobante=2
]]>
	</queryString>
