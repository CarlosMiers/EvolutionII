	<queryString>
		<![CDATA[SELECT
     numero,
     CONCAT(nombres,' ',apellidos) AS nombre,
     cedula,
     fecha,
     unidmed,
     horas,
     inicio,
     fin,
     importe
FROM
     vacaciones
     INNER JOIN empleados ON vacaciones.funcionario = empleados.codigo
     INNER JOIN sucursales ON empleados.sucursal = sucursales.codigo
WHERE numero=$P{nNumero}
ORDER BY idsucursal, empleados.codigo

]]>
	</queryString>
