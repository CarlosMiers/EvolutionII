	<parameter name="cCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT SUM(saldo) AS importesaldo
FROM cuenta_clientes
WHERE cuenta_clientes.cliente=$P{cCliente}
AND cuenta_clientes.saldo>0
AND cuenta_clientes.comprobante=8
GROUP BY cuenta_clientes.cliente
UNION
SELECT SUM(importe) AS importesaldo
FROM saldos_aportes
WHERE  saldos_aportes.socio=$P{cCliente}
GROUP BY saldos_aportes.socio

]]>
	</queryString>
	<field name="importesaldo" class="java.math.BigDecimal"/>
