	<queryString>
		<![CDATA[SELECT
	nombre,
	cedula,
	nombres, apellidos,
	fecha, inicio, fin,
	horas, unidmed, importe, salario
FROM
     `vacaciones` vacaciones INNER JOIN `empleados` empleados ON vacaciones.`funcionario` = empleados.`codigo`
     INNER JOIN `sucursales` sucursales ON empleados.`sucursal` = sucursales.`codigo`
WHERE
     vacaciones.inicio >= $P{FechaI}
     and vacaciones.fin <= $P{FechaF}
ORDER BY sucursal, empleados.codigo]]>
	</queryString>
