	<queryString>
		<![CDATA[SELECT
     numero,
     cedula,
  concat(nombres,' ',apellidos) as nombrecompleto,
 nombres,
     apellidos,
     fecha,
     dias,
     importe,
     unidmed,
     idsucursal,
     sucursales.nombre as nombresucursal
FROM
     jornaleros
     INNER JOIN empleados ON jornaleros.funcionario = empleados.codigo
     INNER JOIN sucursales ON jornaleros.idsucursal = sucursales.codigo
WHERE
     jornaleros.fecha >= $P{FechaI}
     and jornaleros.fecha <= $P{FechaF}
and jornaleros.idsucursal=$P{cSucursal}
ORDER BY idsucursal, empleados.codigo]]>
	</queryString>
