	<queryString>
		<![CDATA[SELECT clientes.nombre as nombrealumno,
clientes.cedula,YEAR(clientes.fechaingreso) AS ingreso,
facultad.nombre AS nombrefacultad,
facultad.decano,facultad.rector,facultad.secretario_general,
carreras.nombre AS nombrecarrera,
materias.nombre AS nombremateria,materias.semestre,
certificado_estudios.periodo,
certificado_estudios.tipoexamen,
certificado_estudios.fechaexamen,
certificado_estudios.codalumno,certificado_estudios.nota,
certificado_estudios.acta,certificado_estudios.estado,
certificado_estudios.materia,certificado_estudios.semestre,
certificado_estudios.codcarrera,
tipo_examen.nombre AS nombreexamen,
materias.cargahoraria,
CASE certificado_estudios.semestre
  WHEN 1 THEN 'PRIMER SEMESTRE'
  WHEN 2 THEN 'SEGUNDO SEMESTRE'
  WHEN 3 THEN 'TERCER SEMESTRE'
  WHEN 4 THEN 'CUARTO SEMESTRE'
  WHEN 5 THEN 'QUINTO SEMESTRE'
  WHEN 6 THEN 'SEXTO SEMESTRE'
  WHEN 7 THEN 'SEPTIMO SEMESTRE'
  WHEN 8 THEN 'OCTAVO SEMESTRE'
  WHEN 9 THEN 'NOVENO SEMESTRE'
  WHEN 10 THEN 'DECIMO SEMESTRE'
  WHEN 11 THEN 'DECIMO PRIMER SEMESTRE'
  WHEN 12 THEN 'DECIMO SEGUNDO SEMESTRE'
END AS nombresemestre,
CASE certificado_estudios.nota
  WHEN 0 THEN 'A'
  WHEN 1 THEN 'UNO'
  WHEN 2 THEN 'DOS'
  WHEN 3 THEN 'TRES'
  WHEN 4 THEN 'CUATRO'
  WHEN 5 THEN 'CINCO'
END AS nombrenota
FROM facultad,carreras,materias,certificado_estudios,tipo_examen,clientes
WHERE facultad.codigo=carreras.facultad
AND carreras.codigo=materias.idcarrera
AND materias.idmateria=certificado_estudios.materia
AND materias.idcarrera=certificado_estudios.codcarrera
AND tipo_examen.codigo=certificado_estudios.tipoexamen
and clientes.codigo=certificado_estudios.codalumno
AND carreras.codigo=$P{cCarrera}
AND certificado_estudios.codalumno=$P{cAlumno}
ORDER BY certificado_estudios.semestre ASC]]>
	</queryString>
