	<queryString>
		<![CDATA[SELECT numero,socio,clientes.nombre,cabecera_descuento_documentos.fecha,moneda,emision,nrodocumento,
cargobanco,monto_documento,librador,monedas.etiqueta,bancos_plaza.nombre as nombrebanco,
vencimiento,tasa,plazo,detalle_descuento_documentos.compra,descuento,valor_actual,acreedor,gastos,
valor_actual-gastos as desembolso
FROM cabecera_descuento_documentos
INNER JOIN detalle_descuento_documentos
ON numero=ndescuento
INNER JOIN clientes
ON clientes.codigo=socio
INNER JOIN monedas
ON monedas.codigo=cabecera_descuento_documentos.moneda
INNER JOIN bancos_plaza
ON bancos_plaza.codigo=detalle_descuento_documentos.cargobanco
WHERE cabecera_descuento_documentos.fecha BETWEEN $P{FechaInicio} AND $P{FechaFinal}
ORDER BY fecha]]>
	</queryString>
