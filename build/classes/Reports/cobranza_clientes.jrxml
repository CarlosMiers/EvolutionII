	<parameter name="cMoneda" class="java.lang.String"/>
	<parameter name="cCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 'A' AS origen,nroboleta,fechapago,cuenta_clientes.cliente,cuenta_clientes.cuota,cuenta_clientes.numerocuota,
cuenta_clientes.amortiza,
cuenta_clientes.minteres,pagoexpress.gastos,pagoexpress.mora,pagoexpress.monto,clientes.nombre,cuenta_clientes.comprobante,
pagoexpress.punitorio,comprobantes.nombre AS nombreprestamo,0.00 as descuentos,cuenta_clientes.documento,
DATEDIFF(fechapago, cuenta_clientes.vencimiento) AS diamora
FROM pagoexpress
INNER JOIN clientes
ON clientes.codigo=pagoexpress.cliente
INNER JOIN cuenta_clientes
ON cuenta_clientes.iddocumento=pagoexpress.iddocumento
INNER JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
WHERE fechapago BETWEEN $P{dFechaInicio} AND $P{dFechaFinal}
and pagoexpress.cliente=$P{cCliente}
UNION
SELECT 'B' AS origen,CONVERT(numero, CHAR(15)) AS nroboleta,cobranzas.fecha AS fechapago,cobranzas.cliente,detalle_cobranzas.cuota,detalle_cobranzas.numerocuota,
detalle_cobranzas.amortiza,
detalle_cobranzas.minteres,detalle_cobranzas.gastos_cobranzas AS gastos,detalle_cobranzas.mora,detalle_cobranzas.pago AS monto,
clientes.nombre,detalle_cobranzas.comprobante,
detalle_cobranzas.punitorio,comprobantes.nombre AS nombreprestamo,descuentos,cuenta_clientes.documento,
detalle_cobranzas.diamora
FROM cobranzas
INNER JOIN clientes
ON clientes.codigo=cobranzas.cliente
INNER JOIN detalle_cobranzas
ON detalle_cobranzas.iddetalle=cobranzas.idpagos
INNER JOIN comprobantes
ON comprobantes.codigo=detalle_cobranzas.comprobante
INNER JOIN cuenta_clientes
ON cuenta_clientes.iddocumento=detalle_cobranzas.idfactura
WHERE cobranzas.fecha BETWEEN $P{dFechaInicio} AND $P{dFechaFinal}
AND cobranzas.moneda=$P{cMoneda}
AND cobranzas.cliente=$P{cCliente}
ORDER BY origen,documento,cuota]]>
	</queryString>
