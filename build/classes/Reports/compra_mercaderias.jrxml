	<queryString>
		<![CDATA[SELECT nrofactura,cabecera_compras.fecha,sucursal,cabecera_compras.proveedor,exentas,
gravadas10,gravadas5,totalneto,moneda,
cabecera_compras.timbrado,cabecera_compras.vencetimbrado,sucursales.nombre AS nombresucursal,
proveedores.nombre AS nombreproveedor,monedas.nombre AS nombremoneda,
proveedores.ruc,
productos.nombre AS nombreproducto,comprobantes.nombre AS nombrecomprobante,
detalle_compras.codprod,detalle_compras.cantidad,
detalle_compras.prcosto,detalle_compras.porcentaje,
detalle_compras.impiva,detalle_compras.monto
 FROM cabecera_compras
 LEFT JOIN sucursales
 ON sucursales.codigo=cabecera_compras.sucursal
 LEFT JOIN proveedores
 ON proveedores.codigo=cabecera_compras.proveedor
 LEFT JOIN monedas
 ON monedas.codigo=cabecera_compras.moneda
 LEFT JOIN comprobantes
 ON comprobantes.codigo=cabecera_compras.comprobante
 LEFT JOIN detalle_compras
 ON detalle_compras.dreferencia=cabecera_compras.creferencia
 LEFT JOIN productos
 ON productos.codigo=detalle_compras.codprod
 WHERE cabecera_compras.creferencia=$P{cReferencia}
 ORDER BY nrofactura
]]>
