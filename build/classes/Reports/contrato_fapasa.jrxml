	<queryString>
		<![CDATA[SELECT numero,prestamos.fecha,prestamos.numero,prestamos.socio,clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,prestamos.totalprestamo,prestamos.plazo,monedas.etiqueta,monedas.nombre AS nombremoneda,SUM(totalprestamo+interes) AS montopagare,
clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,detalle_prestamo.vence,
DAY(prestamos.fecha) AS dia,
YEAR(prestamos.fecha) AS periodo,
CASE MONTH(prestamos.fecha)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes,
DAY(detalle_prestamo.vence) AS vencedia,
CASE MONTH(detalle_prestamo.vence)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS vencemes,
YEAR(detalle_prestamo.vence) AS venceperiodo
FROM prestamos
LEFT JOIN clientes
ON clientes.codigo=prestamos.socio
LEFT JOIN monedas
ON monedas.codigo=prestamos.moneda
LEFT JOIN detalle_prestamo
ON detalle_prestamo.nprestamo=prestamos.numero
WHERE prestamos.numero=$P{nNumeroPrestamo}
GROUP BY prestamos.numero]]>
	</queryString>
