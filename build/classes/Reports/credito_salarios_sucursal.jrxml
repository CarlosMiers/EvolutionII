	<queryString>
		<![CDATA[SELECT
idcontrol,idsucursal,
     concat(nombres,' ',apellidos) as nombrecompleto,
     cedula,
     fecha,
     importe,concepto,
     sucursales.nombre as nombresucursal,
     concepto_salarios.nombre as nombreconcepto
FROM  credito_salarios
     INNER JOIN empleados
 ON credito_salarios.funcionario = empleados.codigo
     INNER JOIN sucursales
 ON credito_salarios.idsucursal = sucursales.codigo
     INNER JOIN concepto_salarios
 ON credito_salarios.concepto= concepto_salarios.codigo
WHERE  credito_salarios.fecha >= $P{FechaI}
     and credito_salarios.fecha <= $P{FechaF}
ORDER BY idsucursal, credito_salarios.concepto,empleados.codigo]]>
	</queryString>
