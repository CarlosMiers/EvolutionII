	<parameter name="nCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT cuenta_clientes.`minteres`,cuenta_clientes.`comprobante`,cuenta_clientes.`documento`,cliente,clientes.`nombre` AS nombrecliente,cuenta_clientes.`totalconcedido`,
cuenta_clientes.`fecha`,cuenta_clientes.`vencimiento`,cuenta_clientes.`saldo`,
cuenta_clientes.`numerocuota`,cuenta_clientes.`cuota`,cuenta_clientes.`comprobante`,comprobantes.`nombre` AS nombrecomprobante,
DATEDIFF(CURDATE(),vencimiento) AS diasmora,cuenta_clientes.`tasaoperativa`
FROM cuenta_clientes
INNER JOIN clientes
ON clientes.codigo=cuenta_clientes.`cliente`
INNER JOIN comprobantes
ON comprobantes.`codigo`=cuenta_clientes.`comprobante`
WHERE cuenta_clientes.`saldo`>0
AND cliente= $P{nCliente}   AND cuenta_clientes.`comprobante`>7
ORDER BY cuenta_clientes.`comprobante`,cuenta_clientes.`documento`,cuenta_clientes.`vencimiento`]]>
	</queryString>
