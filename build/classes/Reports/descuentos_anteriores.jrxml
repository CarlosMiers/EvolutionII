	<parameter name="cCliente" class="java.lang.String"/>
	<parameter name="FechaI" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT YEAR(fecha_cobro)AS anio,MONTH(fecha_cobro) AS mes,SUM(enviado) AS tenviado,
CURDATE()-fecha_cobro AS dias,
SUM(cobrado) AS tcobrado,
SUM(enviado-cobrado) AS tsaldo,
CASE MONTH(fecha_cobro)
WHEN 1 THEN 'ENE'
WHEN 2 THEN 'FEB'
WHEN 3 THEN 'MAR'
WHEN 4 THEN 'ABR'
WHEN 5 THEN 'MAY'
WHEN 6 THEN 'JUN'
WHEN 7 THEN 'JUL'
WHEN 8 THEN 'AGO'
WHEN 9 THEN 'SET'
WHEN 10 THEN 'OCT'
WHEN 11 THEN 'NOV'
WHEN 12 THEN 'DIC'
END AS nombremes
FROM cierre_cobranzas
WHERE fecha_cobro<=$P{FechaI}
AND cliente=$P{cCliente}
GROUP BY anio,mes
ORDER BY dias  LIMIT 6]]>
	</queryString>
