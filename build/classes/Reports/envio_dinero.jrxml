	<queryString>
		<![CDATA[SELECT numero,fecha,cajero,servicio,
productos.nombre AS nombreproducto,
cajas.nombre AS nombrecaja,
concepto,destino,importe,comision,total
FROM servicios
LEFT JOIN cajas
