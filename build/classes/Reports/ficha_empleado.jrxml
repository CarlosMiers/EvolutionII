	<queryString>
		<![CDATA[SELECT
    empleados.codigo as nro,
    empleados.apellidos as apellidos,
    empleados.nombres as nombres,
    empleados.cedula as cedula,
    empleados.sexo as sexo,
    empleados.conyugue as conyugue,
    empleados.estado_civil as civil,
    (CASE WHEN empleados.foto=null THEN '' ELSE empleados.foto END) as foto,
    empleados.fechanacimiento as nacimiento,
    localidades.nombre as localidad,
    paises.nombre as nacionalidad,
    empleados.direccion as direccion,
    empleados.telefono as telefono,
    empleados.celular as celular,
    empleados.telefono_urgencia as urgencia,
    empleados.e_mail as email,
    profesiones.nombre as profesion,
    sucursales.nombre as sucursal,
    empleados.salario as salario,
    departamento_laboral.Nombre as departamento,
    secciones.Nombre as seccion,
    cargos.nombre as cargo,
    (CASE WHEN empleados.ips=0 THEN 'No' ELSE 'Si' END ) as ips,
    (CASE WHEN empleados.bonificacion=0 THEN 'No' ELSE 'Si' END )as bonificacion,
    empleados.adicionalxformacion as adicional,
    (CASE WHEN empleados.nroseguroips=null THEN '' ELSE empleados.nroseguroips END) as nroips,
    empleados.giraduria as giraduria,
    giradurias.nombre as nombregiraduria,
    empleados.fecha_ingreso as ingreso,
    (CASE WHEN empleados.tipo_salario=1 THEN 'HORA'
        WHEN empleados.tipo_salario=2 THEN 'DIARIO'
        WHEN empleados.tipo_salario=3 THEN 'QUINCENAL'
        ELSE 'MENSUAL' END) as tiposalario,
	(CASE WHEN empleados.academia=1 THEN 'ESCOLAR'
        WHEN empleados.academia=2 THEN 'SECUNDARIO'
        WHEN empleados.academia=3 THEN 'UNIVERSITARIO'
        ELSE 'MASTERADO/DOCTORADO' END) as academico,
	(CASE WHEN empleados.espanol=1 THEN 'Español\n' ELSE '' END) as espanol,
    (CASE WHEN empleados.guarani=1 THEN 'Guaraní\n' ELSE '' END) as guarani,
    (CASE WHEN empleados.portugues=1 THEN 'Portugués\n' ELSE '' END) as portugues,
    (CASE WHEN empleados.aleman=1 THEN 'Aleman\n' ELSE '' END) as aleman,
    (CASE WHEN empleados.otros=1 THEN 'Otros' ELSE '' END) as otros
FROM empleados
INNER JOIN localidades on localidades.codigo=empleados.localidad
INNER JOIN paises on paises.codigo=empleados.nacionalidad
INNER JOIN sucursales on sucursales.codigo=empleados.sucursal
INNER JOIN profesiones on profesiones.codigo=empleados.profesion
LEFT JOIN departamento_laboral on departamento_laboral.codigo=empleados.departamento
INNER JOIN secciones on secciones.codigo=empleados.seccion
INNER JOIN cargos on cargos.Codigo=empleados.cargo
INNER JOIN giradurias on giradurias.codigo=empleados.giraduria
WHERE empleados.codigo=$P{Emp}
ORDER BY empleados.giraduria,empleados.sucursal, empleados.departamento,empleados.seccion,empleados.apellidos]]>
	</queryString>
