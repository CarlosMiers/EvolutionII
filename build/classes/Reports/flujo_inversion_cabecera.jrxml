	<queryString>
		<![CDATA[SELECT t.codigo,t.nombre AS nombretitulo,t.pagointeres,t.tasa,t.nomalias,
t.fechaemision,t.vencimiento,t.nominal,t.moneda,t.nombreprograma AS planglobal,
mo.nombre AS nombremoneda,e.calificadora,
e.calificacion,ca.nombre AS nombrecalificadora
FROM titulos t
LEFT JOIN monedas mo
ON mo.codigo=t.moneda
LEFT JOIN emisores e
ON e.codigo=t.empresa
LEFT JOIN calificadoras ca
ON ca.codigo=e.calificadora
WHERE t.nomalias=$P{cSerie}

]]>
