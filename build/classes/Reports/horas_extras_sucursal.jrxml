	<queryString>
		<![CDATA[SELECT
numero,
     cedula,
     idsucursal,
     concat(empleados.nombres,' ',empleados.apellidos) as nombrecompleto,
     nombres,
     apellidos,
     fecha,
     importe,
     unidmed,
      horas,
     sucursales.nombre as nombresucursal
FROM
     horas_extras
     INNER JOIN empleados ON horas_extras.funcionario = empleados.codigo
     INNER JOIN sucursales ON horas_extras.idsucursal = sucursales.codigo
WHERE
     horas_extras.fecha >= $P{FechaI}
     and horas_extras.fecha <= $P{FechaF}
and horas_extras.idsucursal=$P{cSucursal}
ORDER BY idsucursal, empleados.codigo]]>
	</queryString>
