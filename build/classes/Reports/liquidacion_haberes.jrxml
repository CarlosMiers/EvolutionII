	<queryString>
		<![CDATA[SELECT desvinculaciones.numero,desvinculaciones.fecha,
empleados.cedula,empleados.direccion,empleados.telefono,
desvinculaciones.inicio,desvinculaciones.fin,desvinculaciones.antiguedad,
desvinculaciones.funcionario,desvinculaciones.tipo_desvinculacion,
desvinculaciones.dias_preaviso,desvinculaciones.salario_ordinario,
desvinculaciones.vacaciones,desvinculaciones.aguinaldo,
desvinculaciones.preaviso,desvinculaciones.indemnizacion,
desvinculaciones.ips,
CASE
    WHEN desvinculaciones.tipo_desvinculacion= 1 THEN "FINALIZACION CONTRATO"
    WHEN desvinculaciones.tipo_desvinculacion= 2 THEN "RENUNCIA"
    WHEN desvinculaciones.tipo_desvinculacion= 3 THEN "DESPIDO JUSTIFICADO"
    WHEN desvinculaciones.tipo_desvinculacion= 4 THEN "DESPIDO INJUSTIFICADO"
    ELSE "JUBILACION"
END AS motivosalida,
CONCAT(empleados.nombres,', ',empleados.apellidos) AS nombreempleado
FROM desvinculaciones
INNER JOIN empleados
ON empleados.codigo=desvinculaciones.funcionario
WHERE desvinculaciones.numero=$P{cNumero}]]>
	</queryString>
