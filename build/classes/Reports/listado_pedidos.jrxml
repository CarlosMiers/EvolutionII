	<queryString>
		<![CDATA[select p.idpedido,p.sucursal,p.totales,p.fecha,
s.nombre as nombresucursal,d.item,d.producto,pd.nombre as nombreproducto,
d.cantidad,d.costo,d.total
from pedidos p
LEFT JOIN sucursales s
ON s.codigo=p.sucursal
LEFT JOIN pedidos_detalle d
ON d.idpedido=p.idpedido
left join productos pd
ON pd.codigo=d.producto
WHERE p.idpedido=$P{nNum}
ORDER BY p.idpedido,d.item
]]>
	</queryString>
