	<queryString>
		<![CDATA[SELECT cabecera_ventas.creferencia,cabecera_ventas.sucursal,sucursales.nombre AS nombresucursal,cabecera_ventas.moneda, monedas.nombre AS nombremoneda,
cabecera_ventas.cliente,clientes.nombre AS nombrecliente,clientes.direccion AS direccioncliente,clientes.ruc AS ruccliente,
clientes.telefono AS telefonocliente,cabecera_ventas.factura,cabecera_ventas.fecha,detalle_ventas.dreferencia,detalle_ventas.codprod,
productos.nombre AS nombreproducto,detalle_ventas.cantidad,detalle_ventas.precio,detalle_ventas.monto
FROM cabecera_ventas
LEFT JOIN sucursales
ON sucursales.codigo=cabecera_ventas.sucursal
LEFT JOIN monedas
ON monedas.codigo=cabecera_ventas.moneda
LEFT JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
LEFT JOIN detalle_ventas
ON detalle_ventas.dreferencia=cabecera_ventas.creferencia
LEFT JOIN productos
ON productos.codigo=detalle_ventas.codprod
WHERE cabecera_ventas.creferencia=$P{cReferencia}]]>
	</queryString>
