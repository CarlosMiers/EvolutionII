	<queryString>
		<![CDATA[SELECT idordencredito,numero,fecha,ordenes_de_credito.sucursal,
nroorden,tasa,socio,plazo,ordenes_de_credito.giraduria,
importe,monto_cuota,interesordinario,primer_vence,ordenes_de_credito.tipo,
descuentos,montoentregar,giradurias.nombre AS nombregiraduria,clientes.nombre AS nombrecliente,
comprobantes.nombre AS nombrecomprobante,sucursales.nombre AS nombresucursal
FROM ordenes_de_credito
LEFT JOIN giradurias
ON giradurias.codigo=ordenes_de_credito.giraduria
INNER JOIN clientes  ON clientes.codigo=ordenes_de_credito.socio
INNER JOIN comprobantes  ON comprobantes.codigo=ordenes_de_credito.tipo
INNER JOIN sucursales  ON sucursales.codigo=ordenes_de_credito.sucursal
WHERE ordenes_de_credito.giraduria=$P{nGiraduria}
AND ordenes_de_credito.fecha BETWEEN $P{FechaI} AND $P{FechaF}
ORDER BY ordenes_de_credito.numero]]>
	</queryString>
