	<queryString>
		<![CDATA[SELECT numero,orden_compra.fecha,sucursal,
orden_compra.proveedor,totalneto,moneda,
sucursales.nombre AS nombresucursal,
proveedores.nombre AS nombreproveedor,
proveedores.ruc,
if(orden_compra.condicion=1,'COMPRA CONTADO','COMPRA CRÉDITO') AS nombrecomprobante,
monedas.nombre AS nombremoneda,
proveedores.ruc,orden_compra.observaciones,
orden_compra.fechaentrega,
productos.nombre AS nombreproducto,
detalle_orden_compra.codprod,detalle_orden_compra.cantidad,
detalle_orden_compra.precio,detalle_orden_compra.impuesto,
detalle_orden_compra.importeiva,detalle_orden_compra.totalitem
 FROM orden_compra
 LEFT JOIN sucursales
 ON sucursales.codigo=orden_compra.sucursal
 LEFT JOIN proveedores
 ON proveedores.codigo=orden_compra.proveedor
 LEFT JOIN monedas
 ON monedas.codigo=orden_compra.moneda
 LEFT JOIN detalle_orden_compra
 ON detalle_orden_compra.iddetalle=orden_compra.numero
 LEFT JOIN productos
 ON productos.codigo=detalle_orden_compra.codprod
 WHERE orden_compra.numero=$P{cReferencia}
 ORDER BY numero]]>
	</queryString>
