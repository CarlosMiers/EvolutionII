	<queryString>
		<![CDATA[SELECT prestamos.numero,prestamos.fecha,confirmacion,totalprestamo,importe,prestamos.interes,prestamos.plazo,
ivainteres,prestamos.moneda,socio,clientes.nombre AS nombrecliente,nrocheque,codusuario,
bancos.nombre AS nombrebanco,vendedores.nombre AS nombreasesor,
comprobantes.nombre AS nombreprestamo,netocobrado,
monedas.etiqueta AS nombremoneda,usuarios.first_name as nombreusuario
FROM prestamos
INNER JOIN detalle_forma_pago
ON idprestamos=idmovimiento
INNER JOIN clientes
ON clientes.codigo = prestamos.socio
INNER JOIN bancos
ON bancos.codigo = detalle_forma_pago.banco
INNER JOIN vendedores
ON prestamos.oficial=vendedores.codigo
INNER JOIN comprobantes
ON comprobantes.codigo = prestamos.tipo
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
LEFT JOIN usuarios
ON usuarios.employee_id=prestamos.codusuario
WHERE idmovimiento=$P{Referencia}]]>
	</queryString>
