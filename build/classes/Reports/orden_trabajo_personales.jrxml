	<queryString>
		<![CDATA[SELECT orden_trabajo.numero,orden_trabajo.sucursal,orden_trabajo.fechaemision,
orden_trabajo.tipo,orden_trabajo.solicitadopor,orden_trabajo.trabajoarealizar,
orden_trabajo.totalpresupuesto,orden_trabajo.seccion,orden_trabajo.galpon,
orden_trabajo.fechaentregaprevista,orden_trabajo.fechainicio,orden_trabajo.fechaentrega,
secciones.nombre as nombreseccion,sucursales.nombre as nombresucursal,
CONCAT(empleados.nombres,' ',empleados.apellidos) as nombresolicitante,
detalle_tarea_orden_trabajo.descripcion as tareas
from orden_trabajo
LEFT JOIN detalle_tarea_orden_trabajo
on detalle_tarea_orden_trabajo.dnumero=orden_trabajo.numero
LEFT JOIN sucursales
on sucursales.codigo=orden_trabajo.sucursal
LEFT JOIN secciones
ON secciones.codigo=orden_trabajo.seccion
LEFT JOIN empleados
on empleados.codigo=orden_trabajo.solicitadopor
WHERE orden_trabajo.numero=$P{nNumero}]]>
