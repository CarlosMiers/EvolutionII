	<queryString>
		<![CDATA[SELECT numero,fecha,comercio,socio,plazo,usuarioalta,clientes.nombre AS nombresocio,
clientes.direccion,clientes.ruc,
importe,monto_cuota,usuarios.last_name AS nombreusuario,casas.nombre AS nombrecomercio
FROM ordenes_de_compras
INNER JOIN clientes
ON clientes.codigo=ordenes_de_compras.socio
LEFT JOIN usuarios
ON usuarios.employee_id=ordenes_de_compras.usuarioalta
LEFT JOIN casas
ON casas.codigo=ordenes_de_compras.comercio
WHERE numero=$P{nNumero}]]>
	</queryString>
