	<queryString>
		<![CDATA[SELECT  cabecera_descuento_documentos.numero AS numero,
     cabecera_descuento_documentos.fecha AS fecha,
     cabecera_descuento_documentos.moneda AS moneda,
     cabecera_descuento_documentos.`sucursal` AS sucursal,
     cabecera_descuento_documentos.`socio` AS socio,
     cabecera_descuento_documentos.`comision_deudor` AS comision_deudor,
     cabecera_descuento_documentos.`deducciones` AS deducciones,
     cabecera_descuento_documentos.`importe` AS importe,
     cabecera_descuento_documentos.`totalactual` totalactual,
     cabecera_descuento_documentos.`montoiva` AS montoiva,
     cabecera_descuento_documentos.`autorizado_por` AS autorizado_por,
     cabecera_descuento_documentos.`totaldescuento` AS totaldescuento,
     cabecera_descuento_documentos.`oficialcta` AS oficialcta,
     cabecera_descuento_documentos.`observacion` AS observacion,
     vendedores.nombre as nombreoficial,
     clientes.nombre as nombrecliente,
     clientes.telefono as telefono,
     clientes.direccion,
     clientes.ruc,
     monedas.nombre as nombremoneda,
     detalle_forma_pago.nrocheque as nrocheque,
     detalle_forma_pago.netocobrado as netocobrado,
     bancos.nombre as cuentacte
FROM  cabecera_descuento_documentos
INNER JOIN vendedores
ON cabecera_descuento_documentos.asesor=vendedores.codigo
INNER JOIN clientes
ON cabecera_descuento_documentos.socio=clientes.codigo
INNER JOIN monedas
ON cabecera_descuento_documentos.moneda=monedas.codigo
INNER JOIN detalle_forma_pago
ON cabecera_descuento_documentos.idprestamos=detalle_forma_pago.idmovimiento
INNER JOIN bancos
ON bancos.codigo=detalle_forma_pago.banco
where cabecera_descuento_documentos.idprestamos=$P{dNumero}]]>
	</queryString>
