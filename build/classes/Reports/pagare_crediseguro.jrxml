	<queryString>
		<![CDATA[SELECT numero,prestamos.fecha,socio,clientes.nombre,totalprestamo,monedas.etiqueta,monedas.nombre as nombremoneda,
clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,clientes.conyugue,clientes.cedulaconyugue
FROM prestamos
INNER JOIN clientes
ON clientes.codigo=prestamos.socio
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
WHERE prestamos.numero=$P{nNumeroPrestamo}]]>
	</queryString>
