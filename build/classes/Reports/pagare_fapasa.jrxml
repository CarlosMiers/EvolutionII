	<parameter name="nNumeroPrestamo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT numero,prestamos.fecha,prestamos.socio,clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,prestamos.totalprestamo,monedas.etiqueta,monedas.nombre AS nombremoneda,SUM(totalprestamo+interes) AS montopagare,
clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,detalle_prestamo.vence,
DAY(prestamos.fecha) AS dia,
YEAR(prestamos.fecha) AS periodo,
CASE MONTH(prestamos.fecha)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes,
DAY(detalle_prestamo.vence) AS vencedia,
CASE MONTH(detalle_prestamo.vence)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS vencemes,
YEAR(detalle_prestamo.vence) AS venceperiodo,
(SELECT MAX(DAY(vence))
FROM detalle_prestamo
WHERE nprestamo=numero AND prestamos.plazo=nrocuota) diavence,
(SELECT MAX(MONTH(vence))
FROM detalle_prestamo
WHERE nprestamo=numero AND prestamos.plazo=nrocuota) mesvence,
(SELECT MAX(YEAR(vence))
FROM detalle_prestamo
WHERE nprestamo=numero AND prestamos.plazo=nrocuota) anualvence,
garantia_aval.idprestamo AS garantiaidpres,
(SELECT clientes.nombre
 FROM clientes
 WHERE clientes.codigo=garantia_aval.codcliente) codenombre,
 (SELECT clientes.direccion
 FROM clientes
 WHERE clientes.codigo=garantia_aval.codcliente) codedireccion,
  (SELECT clientes.telefono
 FROM clientes
 WHERE clientes.codigo=garantia_aval.codcliente) codetelefono,
  (SELECT clientes.ruc
 FROM clientes
 WHERE clientes.codigo=garantia_aval.codcliente) coderuc
FROM prestamos
LEFT JOIN clientes
ON clientes.codigo=prestamos.socio
LEFT JOIN monedas
ON monedas.codigo=prestamos.moneda
LEFT JOIN detalle_prestamo
ON detalle_prestamo.nprestamo=prestamos.numero
LEFT JOIN garantia_aval
ON garantia_aval.idprestamo=prestamos.numero
WHERE prestamos.numero=$P{nNumeroPrestamo}
GROUP BY prestamos.numero]]>
	</queryString>
