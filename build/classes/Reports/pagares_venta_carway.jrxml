	<queryString>
		<![CDATA[select cabecera_ventas.creferencia,cabecera_ventas.fecha,cabecera_ventas.factura,cabecera_ventas.vencimiento,cliente,clientes.nombre,clientes.ruc,clientes.direccion,clientes.telefono,cabecera_ventas.totalneto,
cabecera_ventas.financiado,cabecera_ventas.nombregarante,cabecera_ventas.direcciongarante,cabecera_ventas.cedulagarante,
DAY(cabecera_ventas.fecha) AS dia,
YEAR(cabecera_ventas.fecha) AS periodo,
CASE MONTH(cabecera_ventas.fecha)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes
FROM cabecera_ventas
LEFT JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
WHERE cabecera_ventas.creferencia=$P{cReferencia}]]>
	</queryString>
