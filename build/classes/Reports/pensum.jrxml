	<queryString>
		<![CDATA[SELECT facultad.nombre AS nombrefacultad,
carreras.nombre AS nombrecarrera,
semestres.nombre AS nombresemestre,
materias.semestre,materias.cargahoraria,
materias.nombre AS nombremateria,
materias.idmateria,
carreras.titulo
FROM facultad
LEFT JOIN carreras
ON carreras.facultad=facultad.codigo
LEFT JOIN materias
ON materias.idcarrera=carreras.codigo
LEFT JOIN semestres
ON semestres.semestre=materias.semestre
WHERE carreras.codigo=$P{cCarrera}
AND semestres.idcarrera=$P{cCarrera}
ORDER BY materias.semestre,materias.idmateria
]]>
	</queryString>
