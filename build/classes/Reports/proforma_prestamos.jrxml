	<parameter name="nNumeroPrestamo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT numero,proformas.fecha,plazo,totalprestamo,importe,deducciones,socio,codusuario,
proformas.tipo,comision_deudor,gastos_escritura,monto_entregar,oficial,proformas.interes,
ivainteres,comision_deudor,nrocuota,vence,capital,amortiza,
dias,minteres,ivaxinteres,monto,moneda,
clientes.nombre,clientes.direccion,clientes.telefono,clientes.ruc,
vendedores.nombre AS nombreasesor,
monedas.nombre AS nombremoneda,comprobantes.nombre AS nombreprestamo,
usuarios.last_name as nombreusuario
FROM proformas
INNER JOIN detalle_proformas
ON numero=nprestamo
INNER JOIN clientes
ON clientes.codigo=proformas.socio
INNER JOIN monedas
ON monedas.codigo=proformas.moneda
INNER JOIN comprobantes
ON comprobantes.codigo=proformas.tipo
INNER JOIN vendedores
ON vendedores.codigo=proformas.oficial
INNER JOIN usuarios
ON usuarios.employee_id=proformas.codusuario
WHERE numero=$P{nNumeroPrestamo}
ORDER BY nrocuota]]>
	</queryString>
