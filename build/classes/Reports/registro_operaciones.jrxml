	<queryString>
		<![CDATA[SELECT cabecera_asientos.numero, cabecera_asientos.fecha, detalle_asientos.asi_numero,
detalle_asientos.asi_codigo, detalle_asientos.asi_descri,detalle_asientos.impdebe, detalle_asientos.imphaber,sucursales.codigo, sucursales.nombre as nombresucursal,plan.nombre as nombrecuenta
FROM cabecera_asientos
LEFT JOIN detalle_asientos
ON detalle_asientos.asi_asient=cabecera_asientos.numero
LEFT JOIN plan
ON plan.codigo=detalle_asientos.asi_codigo
LEFT JOIN sucursales
ON sucursales.codigo=cabecera_asientos.sucursal
WHERE
IF($P{cSucursal}>0,cabecera_asientos.sucursal=$P{cSucursal},TRUE)
AND cabecera_asientos.numero>=$P{nInicio}
AND cabecera_asientos.numero<=$P{nFinal}
AND cabecera_asientos.fecha BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
ORDER BY cabecera_asientos.numero]]>
	</queryString>
