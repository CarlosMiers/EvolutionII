	<subDataset name="New Dataset 1" uuid="7c1959f9-e0b0-4f6b-b173-84880d2ad295">
		<queryString>
			<![CDATA[SELECT codigo,nombre,
(SELECT SUM(monto)
FROM pagoexpress,clientes,cuenta_clientes
WHERE  pagoexpress.cliente=clientes.codigo
AND vendedores.codigo=clientes.asesor
AND pagoexpress.iddocumento = cuenta_clientes.iddocumento
AND pagoexpress.fechapago BETWEEN '2016-08-01' AND '2016-08-17'
AND moneda=1
AND comprobante=8
GROUP BY vendedores.codigo
) boca,
(SELECT SUM(pago)
FROM cobranzas,clientes,detalle_cobranzas
WHERE  cobranzas.cliente=clientes.codigo
AND vendedores.codigo=clientes.asesor
AND cobranzas.idpagos=detalle_cobranzas.iddetalle
AND cobranzas.fecha BETWEEN '2016-08-01' AND '2016-08-17'
AND cobranzas.moneda=1
AND detalle_cobranzas.comprobante=8
GROUP BY vendedores.codigo
) caja
FROM vendedores
WHERE vendedores.estado=1
GROUP BY vendedores.codigo]]>
		</queryString>
