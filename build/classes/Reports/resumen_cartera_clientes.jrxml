	<queryString>
		<![CDATA[SELECT cliente,documento,comprobante,clientes.nombre,fecha,capital,comprobantes.nombre AS tipocartera,categoria_clientes.nombre AS categoria,numerocuota,cuota,vencimiento,totalconcedido,
MAX(IF(cuenta_clientes.saldo>0,cuenta_clientes.diasmora,0)) AS diasatraso,SUM(cuenta_clientes.saldo) AS saldototal,SUM(amortiza) AS saldocapital,
(select COUNT(saldo) FROM cuenta_clientes B WHERE cuenta_clientes.creferencia=B.creferencia AND saldo=0) cuotapag
FROM cuenta_clientes
INNER JOIN clientes
ON clientes.codigo=cuenta_clientes.cliente
INNER JOIN categoria_clientes
ON categoria_clientes.codigo=clientes.categoria
INNER JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
WHERE cuenta_clientes.saldo<>0
and cuenta_clientes.moneda=$P{cMoneda}
GROUP BY documento,cliente,comprobante
ORDER BY comprobante,cliente,vencimiento]]>
	</queryString>
