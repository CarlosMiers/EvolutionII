	<parameter name="cMoneda" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT cobranzas.numero,cobranzas.sucursal,cobranzas.cobrador,
cobranzas.fecha,cobranzas.cliente,
cobranzas.moneda,cobranzas.totalpago,clientes.nombre AS nombrecliente,
sucursales.nombre AS nombresucursal,monedas.nombre AS nombremoneda,
COALESCE(efe.efectivo,0) AS efectivo,
COALESCE(che.cheque,0) AS cheque
FROM cobranzas
LEFT JOIN clientes
ON clientes.codigo=cobranzas.cliente
LEFT JOIN sucursales
ON sucursales.codigo=cobranzas.sucursal
LEFT JOIN monedas
ON monedas.codigo=cobranzas.moneda
LEFT JOIN (SELECT idmovimiento, SUM(detalle_forma_cobro.netocobrado) AS efectivo
FROM detalle_forma_cobro
WHERE forma=1
GROUP BY detalle_forma_cobro.idmovimiento) efe ON efe.idmovimiento=cobranzas.idpagos
LEFT JOIN (SELECT idmovimiento, SUM(detalle_forma_cobro.netocobrado) AS cheque
FROM detalle_forma_cobro
WHERE forma=2
GROUP BY detalle_forma_cobro.idmovimiento) che ON che.idmovimiento=cobranzas.idpagos
WHERE cobranzas.fecha BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
AND cobranzas.moneda=$P{cMoneda}
ORDER BY cobranzas.fecha
]]>
