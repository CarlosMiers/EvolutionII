	<parameter name="FechaFinal" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT p.fechacierre,p.numerobolsa,p.comprador AS cuentacliente,c.comitente,"CM" AS tipo,p.emisor,e.nomalias as nombremisor,
t.nomalias AS serie,p.vencimiento,p.cantidad,p.titulo,
t.nombre as nombretitulo,p.mercado,
IF(p.mercado=1,'P','S') AS nmercado,
p.moneda,m.etiqueta,
p.valor_nominal,p.precio,p.valor_inversion
FROM precierre_operacion p
INNER JOIN emisores e
ON e.codigo=p.emisor
INNER JOIN titulos t
ON t.codigo=p.titulo
INNER JOIN monedas m
ON m.codigo=p.moneda
INNER JOIN clientes c
ON c.codigo=p.comprador
WHERE p.fechacierre BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
AND p.comprador<>0
AND p.operacion=1
UNION
SELECT p.fechacierre,p.numerobolsa,p.vendedor AS cuentacliente,c.comitente,"VN" AS tipo,p.emisor,e.nomalias as nombremisor,
t.nomalias AS serie,p.vencimiento,p.cantidad,p.titulo,t.nombre as nombretitulo,
p.mercado,
IF(p.mercado=1,'P','S') AS nmercado,
p.moneda,m.etiqueta,
p.valor_nominal,p.precio,p.valor_inversion
FROM precierre_operacion p
INNER JOIN emisores e
ON e.codigo=p.emisor
INNER JOIN titulos t
ON t.codigo=p.titulo
INNER JOIN monedas m
ON m.codigo=p.moneda
INNER JOIN clientes c
ON c.codigo=p.vendedor
WHERE p.fechacierre BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
AND p.vendedor<>0
AND p.operacion=1
ORDER BY moneda,numerobolsa]]>
	</queryString>
