	<queryString>
		<![CDATA[SELECT comprobantes.nombre,sum(saldo) as deuda
FROM cuenta_clientes
LEFT JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
WHERE cuenta_clientes.cliente=$P{cCliente}
AND cuenta_clientes.saldo<>0
GROUP BY cuenta_clientes.comprobante]]>
	</queryString>
