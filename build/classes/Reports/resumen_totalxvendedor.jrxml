	<queryString>
		<![CDATA[SELECT COUNT(vendedor) AS cantidadventas,
cabecera_ventas.vendedor,vendedores.nombre AS nombrevendedor,
sucursales.nombre as nombresucursal,
monedas.nombre as nombremoneda,
SUM(totalneto) AS totalventas
FROM cabecera_ventas
LEFT JOIN vendedores
ON vendedores.codigo=cabecera_ventas.vendedor
LEFT JOIN sucursales
ON sucursales.codigo=cabecera_ventas.sucursal
LEFT JOIN monedas
ON monedas.codigo=cabecera_ventas.moneda
WHERE cabecera_ventas.fecha
BETWEEN $P{FechaInicio} AND $P{FechaFinal}
AND cabecera_ventas.sucursal=$P{cSucursal}
AND IF($P{cVendedor}<>0,cabecera_ventas.vendedor=$P{cVendedor},TRUE)
AND IF($P{cMoneda}<>0,cabecera_ventas.moneda=$P{cMoneda},TRUE)
GROUP BY cabecera_ventas.vendedor
ORDER BY totalventas DESC
]]>
	</queryString>
