	<parameter name="cIdsolicitud" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT solicitud_locacion.idsolicitud,garante_alquiler.idcliente,clientes.codigo, clientes.nombre, clientes.ruc, clientes.fechanacimiento,
CASE clientes.estadocivil
  WHEN "C" THEN "CASADO/A"
  WHEN "S" THEN "SOLTERO/A"
  WHEN "SE" THEN "SEPARADO/A"
  WHEN "V" THEN "VIUDO/A"
END AS estadocivilcode,
clientes.direccion,clientes.celular AS telefonofijocode, clientes.celular AS telefonomovilcode,
localidades.codigo AS codigocode,localidades.nombre AS localidadcode, barrios.codigo AS barriocode, barrios.nombre AS barrionombrecode,
clientes.mail, clientes.direccionlaboral, clientes.lugartrabajo, clientes.cargolaboral,  clientes.salario
FROM garante_alquiler
LEFT JOIN solicitud_locacion
ON solicitud_locacion.idsolicitud=garante_alquiler.idcliente
LEFT JOIN clientes
ON clientes.codigo=garante_alquiler.idcliente
LEFT JOIN localidades
ON localidades.codigo=clientes.localidad
LEFT JOIN barrios
ON barrios.codigo=clientes.barrio
WHERE solicitud_locacion.idsolicitud=$P{cIdsolicitud}
ORDER BY garante_alquiler.idcliente;]]>
	</queryString>
