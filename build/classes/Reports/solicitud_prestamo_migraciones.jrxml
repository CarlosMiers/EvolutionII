	<queryString>
		<![CDATA[SELECT prestamos.numero,prestamos.fecha,prestamos.totalprestamo,prestamos.importe,prestamos.deducciones,
prestamos.socio,prestamos.codusuario,prestamos.tipo,comprobantes.nombre AS nombrecomprobante,prestamos.comision_deudor,prestamos.gastos_escritura,prestamos.monto_entregar,prestamos.monto_cuota,
prestamos.oficial,prestamos.interes,prestamos.plazo,
ivainteres,comision_deudor,nrocuota,vence,capital,amortiza,
dias,minteres,ivaxinteres,monto,moneda,
clientes.nombre AS nombrecliente,clientes.direccion AS direccioncliente,clientes.telefono AS telefonocliente,clientes.ruc AS ruccliente,
vendedores.nombre AS nombreasesor,
monedas.nombre AS nombremoneda,comprobantes.nombre AS nombreprestamo,
usuarios.last_name AS nombreusuario
FROM prestamos
INNER JOIN detalle_prestamo
ON numero=nprestamo
INNER JOIN clientes
ON clientes.codigo=prestamos.socio
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
INNER JOIN comprobantes
ON comprobantes.codigo=prestamos.tipo
INNER JOIN vendedores
ON vendedores.codigo=prestamos.oficial
INNER JOIN usuarios
ON usuarios.employee_id=prestamos.codusuario
WHERE numero=$P{nNumeroPrestamo}
ORDER BY nrocuota]]>
	</queryString>
