	<parameter name="nusuario" class="java.lang.Integer"/>
	<parameter name="dFecha" class="java.util.Date"/>
	<queryString>
		<![CDATA[select "VENTAS" AS tipoingreso,
SUM(IF(cabecera_ventas.comprobante=1, cabecera_ventas.totalneto, 0)) ingresos,
SUM(IF(cabecera_ventas.comprobante=3 AND cabecera_ventas.cliente=1, cabecera_ventas.totalneto, 0)) devoluciones
FROM cabecera_ventas
where fecha=$P{dFecha}
AND idusuario=$P{nusuario}
UNION
select "COBRANZAS",SUM(totalpago) as ingresos,0.00 as devoluciones
from cobranzas
where fecha=$P{dFecha}
AND codusuario=$P{nusuario}
UNION
select "TRANSF.",SUM(importe) as ingresos,0.00 as devoluciones
from apertura
where fecha=$P{dFecha}
AND apertura.usuario=$P{nusuario}
GROUP BY apertura.usuario
]]>
	</queryString>
	<field name="tipoingreso" class="java.lang.String"/>
	<field name="ingresos" class="java.lang.Double">
