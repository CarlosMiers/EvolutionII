	<queryString>
		<![CDATA[SELECT MIN(cabecera_ventas.factura) AS iniciofactura,MAX(cabecera_ventas.factura) AS finfactura,
COUNT(cabecera_ventas.factura) AS cantidadventas,SUM(totalneto) AS totalventa,
SUM(IF(comprobante = 1, totalneto, 0)) contado,
SUM(IF(comprobante = 2, totalneto, 0)) credito,
turno,cajas.nombre as nombrecaja,cabecera_ventas.idusuario,
usuarios.last_name as nombreusuario,cabecera_ventas.fecha
FROM cabecera_ventas
LEFT JOIN cajas
ON cajas.codigo=cabecera_ventas.caja
LEFT JOIN usuarios
ON usuarios.employee_id=cabecera_ventas.idusuario
WHERE caja=$P{caja}
AND turno=$P{turno}
AND fecha=$P{dFecha}
GROUP BY cabecera_ventas.turno,cabecera_ventas.caja
ORDER BY cabecera_ventas.factura]]>
	</queryString>
