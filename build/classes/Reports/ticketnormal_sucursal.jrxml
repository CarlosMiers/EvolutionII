	<queryString>
		<![CDATA[SELECT creferencia,CABECERA_VENTAS.factura,fecha,
vencimiento,cliente,clientes.nombre,clientes.ruc,
exentas,gravadas10,gravadas5,
cabecera_ventas.gravadas10 AS totaliva10,
cabecera_ventas.gravadas5 AS totaliva5,
comprobante,moneda,gravadas10,totalneto,
codprod,detalle_ventas.precio,detalle_ventas.monto,cantidad,
productos.nombre as nombreproducto,supago,
sucambio,COMPROBANTES.nombre as nombrecomprobante,cabecera_ventas.formatofactura,
vendedores.nombre as nombrevendedor,
cabecera_ventas.observacion,cabecera_ventas.centro,
obras.nombre as nombreobra,obras.direccionobra,
obras.contacto
FROM cabecera_ventas
INNER JOIN detalle_ventas
ON detalle_ventas.dreferencia=cabecera_ventas.creferencia
LEFT JOIN obras
ON obras.codigo=cabecera_ventas.centro
INNER JOIN productos
ON productos.codigo=detalle_ventas.codprod
INNER JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
LEFT JOIN vendedores
ON vendedores.codigo=cabecera_ventas.vendedor
LEFT JOIN comprobantes
ON comprobantes.codigo=cabecera_ventas.comprobante
WHERE cabecera_ventas.creferencia=$P{cReferencia}
AND detalle_ventas.monto>0
ORDER BY cabecera_ventas.creferencia]]>
	</queryString>
