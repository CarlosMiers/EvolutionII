	<queryString>
		<![CDATA[SELECT cobranzas.idpagos,cobranzas.fecha,cobranzas.numero,cobranzas.cliente,cobranzas.totalpago,clientes.nombre,clientes.ruc,
detalle_cobranzas.nrofactura,detalle_cobranzas.cuota,detalle_cobranzas.numerocuota,detalle_cobranzas.amortiza,detalle_cobranzas.minteres,detalle_cobranzas.mora,detalle_cobranzas.punitorio,
detalle_cobranzas.gastos_cobranzas,detalle_cobranzas.pago,comprobantes.nombre as nombrecomprobante,
(SELECT importesaldo FROM vista_saldo_aportes
WHERE clientes.codigo=vista_saldo_aportes.socio) aporte,
(SELECT importesaldo FROM vista_saldo_prestamos
WHERE clientes.codigo=vista_saldo_prestamos.cliente) prestamo
FROM clientes
INNER JOIN cobranzas
ON cobranzas.cliente=clientes.codigo
INNER JOIN detalle_cobranzas
ON detalle_cobranzas.iddetalle=cobranzas.idpagos
INNER JOIN comprobantes
ON comprobantes.codigo=detalle_cobranzas.comprobante
WHERE cobranzas.idpagos=$P{cReferencia}]]>
