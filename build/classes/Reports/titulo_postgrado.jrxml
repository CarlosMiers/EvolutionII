	<queryString>
		<![CDATA[SELECT clientes.nombre as nombrealumno,
clientes.cedula,YEAR(clientes.fechaingreso) AS ingreso,
facultad.nombre AS nombrefacultad,
facultad.decano,facultad.rector,facultad.secretario_general,
carreras.nombre AS nombrecarrera,carreras.titulo,
YEAR(clientes.fechaculminacion) AS culminacion,clientes.nroregistro
FROM clientes
LEFT JOIN carreras
ON carreras.codigo=clientes.carrera
LEFT JOIN facultad
ON facultad.codigo=carreras.facultad
WHERE clientes.codigo=$P{cAlumno}
AND carreras.codigo=$P{cCarrera}
ORDER BY clientes.codigo]]>
	</queryString>
