	<queryString>
		<![CDATA[SELECT
     cedula,
     nombres,
     apellidos,
     fecha,
     inicio,
     fin,
     importe,
     unidmed,
horas,
     idsucursal,
      sucursales.nombre as nombresucursal
FROM
     vacaciones
     INNER JOIN empleados ON vacaciones.funcionario = empleados.codigo
     INNER JOIN sucursales ON vacaciones.idsucursal = sucursales.codigo
WHERE
     vacaciones.fecha >= $P{FechaI}
     and vacaciones.fecha <= $P{FechaF}
ORDER BY idsucursal, empleados.codigo
]]>
	</queryString>
