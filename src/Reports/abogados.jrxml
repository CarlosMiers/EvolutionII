	<queryString>
		<![CDATA[SELECT clientes.codigo,clientes.nombre AS nombrecliente,
clientes.cedula,clientes.direccion,clientes.telefono,
clientes.abogado,clientes.situacion,clientes.motivosituacion,
abogados.nombre AS nombreabogado,situacion.nombre AS nombresituacion
