	<queryString>
		<![CDATA[SELECT
    anticipo_aguinaldos.idcontrol,
    empleados.codigo,
     empleados.cedula as cedula,
     concat(empleados.nombres,' ',empleados.apellidos) as nombrecompleto,
     anticipo_aguinaldos.fecha as fechaadelanto,
     anticipo_aguinaldos.importe as importe,
     empleados.giraduria as giraduria,
     empleados.seccion as seccion,
     giradurias.nombre as nombregiraduria,
     secciones.nombre as nombreseccion
FROM
     anticipo_aguinaldos
     INNER JOIN empleados ON anticipo_aguinaldos.funcionario = empleados.codigo
     INNER JOIN giradurias ON giradurias.codigo = empleados.giraduria
     inner join secciones ON secciones.codigo=empleados.seccion
WHERE
     anticipo_aguinaldos.fecha >=$P{FechaI}
     and anticipo_aguinaldos.fecha <= $P{FechaF}
     and empleados.giraduria= $P{giraduria}
ORDER BY empleados.giraduria,empleados.seccion,empleados.codigo]]>
	</queryString>
