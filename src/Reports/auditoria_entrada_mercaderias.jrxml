	<queryString>
		<![CDATA[SELECT numero,ajuste_mercaderias.fecha,horagrabado,
SUM(detalle_ajuste_mercaderias.cantidad*detalle_ajuste_mercaderias.costo) AS totalitem,
COUNT(detalle_ajuste_mercaderias.producto) AS item
FROM ajuste_mercaderias
LEFT JOIN detalle_ajuste_mercaderias
ON detalle_ajuste_mercaderias.dreferencia=ajuste_mercaderias.idreferencia
WHERE ajuste_mercaderias.fecha BETWEEN $P{dFechaIni}
AND $P{dFechaFin}
GROUP BY numero
ORDER BY numero]]>
	</queryString>
