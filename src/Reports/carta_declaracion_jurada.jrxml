	<parameter name="cNombreEmpresa" class="java.lang.String"/>
	<parameter name="nNumero" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT precierre_operacion.fechacierre,comprador,clientes.nombre as nombrecliente, precierre_operacion.fechacierre,
DAY(precierre_operacion.fecha) AS dia,
YEAR(precierre_operacion.fecha) AS periodo,
emisores.nombre as nombreemisor,
titulos.rescnv,
date_format(titulos.fechacnv, "%d/%m/%Y") as fecha_formateada,
CASE MONTH(precierre_operacion.fechacierre)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes
FROM precierre_operacion
LEFT JOIN clientes
ON clientes.codigo=precierre_operacion.comprador
LEFT JOIN emisores
ON emisores.codigo=precierre_operacion.emisor
LEFT JOIN titulos
ON titulos.codigo=precierre_operacion.titulo
WHERE precierre_operacion.numero=$P{nNumero}]]>
	</queryString>
