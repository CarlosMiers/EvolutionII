	<queryString>
		<![CDATA[SELECT clientes.asesor,vendedores.nombre AS nombreasesor,
clientes.nombre AS nombrecliente,
SUM(cuenta_clientes.saldo) AS totaldeuda,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) >1 ,  cuenta_clientes.saldo, 0 ) ) AS mora,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) <=0 ,  cuenta_clientes.saldo, 0 ) ) AS aldia,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) BETWEEN 1 AND 30 ,  cuenta_clientes.saldo, 0 ) ) AS 30dias,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) BETWEEN 31 AND 60 ,  cuenta_clientes.saldo, 0 ) ) AS 60dias,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) BETWEEN 61 AND 90 ,  cuenta_clientes.saldo, 0 ) ) AS 90dias,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento) BETWEEN 91 AND 180 ,  cuenta_clientes.saldo, 0 ) ) AS Masde90,
SUM(  IF( DATEDIFF(CURDATE(),vencimiento)>180 ,  cuenta_clientes.saldo, 0 ) ) AS Masde180,
comprobantes.nombre AS tipo_operacion,monedas.nombre AS nombremoneda
FROM clientes
INNER JOIN cuenta_clientes
ON clientes.codigo=cuenta_clientes.cliente
INNER JOIN vendedores
ON clientes.`asesor`=vendedores.`codigo`
INNER JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
INNER JOIN monedas
ON monedas.codigo= cuenta_clientes.`moneda`
WHERE cuenta_clientes.saldo>0
GROUP BY clientes.codigo
having mora>0
ORDER BY vendedores.codigo,clientes.nombre]]>
