	<queryString>
		<![CDATA[SELECT creferencia,precierre_operacion.numero,negociado,numerobolsa,
precierre_operacion.fecha,validohasta,fechacierre,
precierre_operacion.fechaemision,
day(precierre_operacion.fechacierre) as dia,
year(precierre_operacion.fechacierre) as periodo,
CASE MONTH(precierre_operacion.fechacierre)
  WHEN 1 THEN 'Enero'
  WHEN 2 THEN 'Febrero'
  WHEN 3 THEN 'Marzo'
  WHEN 4 THEN 'Abril'
  WHEN 5 THEN 'Mayo'
  WHEN 6 THEN 'Junio'
  WHEN 7 THEN 'Julio'
  WHEN 8 THEN 'Agosto'
  WHEN 9 THEN 'Setiembre'
  WHEN 10 THEN 'Octubre'
  WHEN 11 THEN 'Noviembre'
  WHEN 12 THEN 'Diciembre'
END AS nombremes,
precierre_operacion.vencimiento,precierre_operacion.comprador,
precierre_operacion.vendedor,(SELECT nombre FROM clientes WHERE comprador=clientes.codigo) nombreclientecomprador,
(SELECT nombre FROM clientes WHERE precierre_operacion.vendedor=clientes.codigo)  nombreclientevendedor ,
(SELECT comitente FROM clientes WHERE precierre_operacion.comprador=clientes.codigo) comitentecomprador,
(SELECT comitente FROM clientes WHERE precierre_operacion.vendedor=clientes.codigo) comitentevendedor,
asesorcompra,asesorventa,
(SELECT nombre FROM vendedores WHERE asesorcompra=vendedores.codigo) nombreasesorcomprador,
(SELECT nombre FROM vendedores WHERE asesorventa=vendedores.codigo)  nombreasesorvendedor,
precierre_operacion.operacion,tipooperacion,
precierre_operacion.mercado,precierre_operacion.moneda,
monedas.nombre AS nombremoneda,
monedas.etiqueta,
emisor,emisores.nombre AS nombreemisor,
emisores.nomalias AS aliasemisor,
titulo,titulos.nombre AS nombretitulo,
titulos.nomalias as aliastitulo,
titulos.pagointeres,
IF(precierre_operacion.cuentapago="","1","2") AS depositadoen,
valor_nominal,cantidad,precio,valor_inversion,
ROUND(precierre_operacion.valor_nominal/titulos.nominal) as ncantidad,
REPLACE(FORMAT(titulos.nominal,0),",", ".") AS nnominal,
precierre_operacion.tasa,plazo,tipoplazo,numerobolsa,
precierre_operacion.base,precierre_operacion.cortes,
precierre_operacion.preciocorte,precierre_operacion.periodopago,
precierre_operacion.custodia,
titulos.rescnv,titulos.resbvpasa,
DATE_FORMAT(titulos.fechacnv, "%d/%m/%Y") AS fecharescnv
 FROM precierre_operacion
 LEFT JOIN monedas
 ON precierre_operacion.moneda=monedas.codigo
 LEFT JOIN emisores
 ON precierre_operacion.emisor=emisores.codigo
 LEFT JOIN titulos
 ON precierre_operacion.titulo=titulos.codigo
 WHERE precierre_operacion.numero= $P{nNumero}
 AND operacion=1 ORDER BY numero]]>
	</queryString>
