	<queryString>
		<![CDATA[SELECT pagos.numero,pagos.recibo,pagos.fecha,pagos.observacion,proveedores.nombre AS nombrebeneficiario,
detalle_forma_pago.nrocheque,detalle_forma_pago.banco,detalle_forma_pago.netocobrado,
detalle_forma_pago.confirmacion,
DAY(pagos.fecha) AS diaemision,
MONTH(pagos.fecha) AS mesemision,
CONVERT(SUBSTR(DATE_FORMAT(pagos.fecha, '%Y'),3,2),DECIMAL) AS anoemision,
DAY(detalle_forma_pago.confirmacion) AS diavence,
MONTH(detalle_forma_pago.confirmacion) AS mesvence,
SUBSTR(DATE_FORMAT(detalle_forma_pago.confirmacion, '%Y'),3,2) AS periodovence
FROM pagos
LEFT JOIN proveedores
ON proveedores.codigo=pagos.proveedor
LEFT JOIN detalle_forma_pago
ON detalle_forma_pago.idmovimiento=pagos.idpagos
WHERE pagos.idpagos=$P{IdPagos}
AND detalle_forma_pago.nrocheque=$P{cCheque}
ORDER BY detalle_forma_pago.nrocheque]]>
	</queryString>
