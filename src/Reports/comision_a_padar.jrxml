	<queryString language="SQL">
		<![CDATA[SELECT comprobantes.nomalias,comprobantes.porcientomora,comprobantes.porcientoperiodo,
vendedor,vendedores.nombre,producto,mes,anual,saldomora,saldoperiodo,ROUND(saldomora*comprobantes.porcientomora/100,0) AS moraxcobrar,
ROUND(saldoperiodo*comprobantes.porcientoperiodo/100,0) AS periodoxcobrar,
moracobrada,periodocobrado,totalcobrado
FROM config_comision
INNER JOIN comprobantes
ON comprobantes.codigo=config_comision.producto
INNER JOIN vendedores
ON vendedores.codigo=config_comision.vendedor]]>
