	<queryString>
		<![CDATA[SELECT
     empleados.cedula as cedula,
	credito_salarios.idcontrol,
     concat(empleados.nombres,' ',empleados.apellidos) as nombrecompleto,
     credito_salarios.fecha as fechaadelanto,
     credito_salarios.importe as importe,
     concepto_salarios.nombre as concepto,
     sucursales.nombre as sucursal
FROM
     credito_salarios
     INNER JOIN empleados ON credito_salarios.funcionario = empleados.codigo
     INNER JOIN concepto_salarios ON credito_salarios.concepto= concepto_salarios.codigo
     INNER JOIN sucursales ON empleados.sucursal = sucursales.codigo
WHERE
     credito_salarios.fecha >=$P{FechaI}
     and credito_salarios.fecha <= $P{FechaF}
ORDER BY empleados.sucursal,credito_salarios.concepto, credito_salarios.funcionario]]>
	</queryString>
