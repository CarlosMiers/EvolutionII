	<parameter name="cCliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT numero,fecha,importe,plazo,
casas.nombre AS nombrecasa
FROM ordenes_de_credito
LEFT JOIN casas
ON casas.codigo=ordenes_de_credito.comercio
WHERE ordenes_de_credito.socio=$P{cCliente}
AND ordenes_de_credito.estado<>'ANULADO'
ORDER BY ordenes_de_credito.fecha DESC
LIMIT 4]]>
	</queryString>
