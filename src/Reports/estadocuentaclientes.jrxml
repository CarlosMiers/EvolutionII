	<queryString>
		<![CDATA[SELECT clientes.codigo,clientes.categoria,clientes.asesor,clientes.nombre AS nombrecliente,
clientes.celular, clientes.direccion, clientes.mail,clientes.lugartrabajo AS nombregiraduria,
clientes.salario,clientes.ruc,clientes.telefono,
cuenta_clientes.documento,cuenta_clientes.fecha,cuenta_clientes.vencimiento,
cuenta_clientes.comprobante,cuenta_clientes.importe,cuenta_clientes.saldo,
cuenta_clientes.numerocuota,cuenta_clientes.cuota,cuenta_clientes.fecha_pago,
comprobantes.nombre AS nombrecomprobante,clientes.cargolaboral
FROM clientes
LEFT JOIN
cuenta_clientes
ON cuenta_clientes.cliente=clientes.codigo
LEFT JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
WHERE clientes.codigo= $P{cCliente}
AND cuenta_clientes.fecha<=$P{FechaI}
ORDER BY cuenta_clientes.cliente,cuenta_clientes.fecha,cuenta_clientes.documento,cuenta_clientes.cuota]]>
	</queryString>
