	<queryString>
		<![CDATA[SELECT creferencia,factura,remisionnro,fecha,vencimiento,cliente,clientes.nombre,clientes.ruc,
clientes.direccion,clientes.telefono,comprobante,formatofactura,ticketold,
cabecera_ventas.exentas AS totalexentas,
cabecera_ventas.gravadas10 totaliva10,
cabecera_ventas.gravadas5 totaliva5,
detalle_ventas.monto,
IFNULL(ROUND(((`cabecera_ventas`.`gravadas10` * `cabecera_ventas`.`cotizacion`) / 11),0),0) AS `iva10`,
IFNULL(ROUND(((`cabecera_ventas`.`gravadas5` * `cabecera_ventas`.`cotizacion`) / 21),0),0) AS `iva5`,
comprobante,moneda,cabecera_ventas.preventa,
COALESCE(iva.importeiva,0) AS importeiva
FROM cabecera_ventas
LEFT JOIN (SELECT detalle_ventas.dreferencia, SUM(ABS(detalle_ventas.monto)/11) AS importeiva
FROM detalle_ventas
WHERE detalle_ventas.monto > 0
GROUP BY detalle_ventas.dreferencia) iva ON iva.dreferencia=cabecera_ventas.creferencia
LEFT JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
INNER JOIN detalle_ventas
ON detalle_ventas.dreferencia=cabecera_ventas.creferencia
WHERE cabecera_ventas.creferencia=$P{cReferencia}
AND detalle_ventas.monto>0
ORDER BY cabecera_ventas.creferencia]]>
	</queryString>
