	<queryString>
		<![CDATA[SELECT cuenta_clientes.creferencia,cuenta_clientes.iddocumento,cuenta_clientes.documento,cuenta_clientes.fecha,
cuenta_clientes.vencimiento,cuenta_clientes.giraduria,cuenta_clientes.comprobante,
cuenta_clientes.importe AS importe,cuenta_clientes.cliente,cuenta_clientes.saldo,
cuenta_clientes.cuota AS cuota,cuenta_clientes.numerocuota,
clientes.nombre AS nombrecliente,
comprobantes.nombre AS nombrecomprobante,comprobantes.nomalias
FROM cuenta_clientes
INNER JOIN CLIENTES
ON clientes.codigo=cuenta_clientes.cliente
INNER JOIN comprobantes
ON comprobantes.codigo=cuenta_clientes.comprobante
 WHERE cuenta_clientes.saldo<>0
 AND cuenta_clientes.cliente= $P{cCliente}
 AND cuenta_clientes.fecha BETWEEN $P{FechaInicio}
AND $P{FechaFinal}
 AND cuenta_clientes.moneda=1
 ORDER BY cuenta_clientes.fecha]]>
	</queryString>
