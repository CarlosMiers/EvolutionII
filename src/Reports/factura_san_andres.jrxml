	<queryString>
		<![CDATA[SELECT creferencia,CABECERA_VENTAS.factura,fecha,vencimiento,cliente,clientes.nombre,clientes.ruc,
exentas,gravadas10,gravadas5,
IFNULL(ROUND(((`cabecera_ventas`.`gravadas10` * `cabecera_ventas`.`cotizacion`) / 11),0),0) AS `totaliva10`,
IFNULL(ROUND(((`cabecera_ventas`.`gravadas5` * `cabecera_ventas`.`cotizacion`) / 21),0),0) AS `totaliva5`,
comprobante,moneda,gravadas10,totalneto,codprod,precio,monto,cantidad,
porcentaje,productos.nombre AS nombreproducto,cabecera_ventas.supago,sucambio,nrotimbrado,
vencimientotimbrado,CAJAS.nombre AS nombrecaja,CAJAS.timbrado,CAJAS.iniciotimbrado,CAJAS.vencetimbrado,
COMPROBANTES.nombre AS nombrecomprobante,cabecera_ventas.formatofactura
FROM cabecera_ventas
INNER JOIN detalle_ventas
ON detalle_ventas.dreferencia=cabecera_ventas.creferencia
INNER JOIN productos
ON productos.codigo=detalle_ventas.codprod
INNER JOIN clientes
ON clientes.codigo=cabecera_ventas.cliente
LEFT JOIN cajas
ON cajas.codigo=cabecera_ventas.caja
INNER JOIN comprobantes
ON comprobantes.codigo=cabecera_ventas.comprobante
WHERE cabecera_ventas.creferencia=$P{cReferencia}]]>
	</queryString>
