	<queryString>
		<![CDATA[SELECT	productos.codigo AS codigo,
    IFNULL(productos.codigobarra,'-') AS codigobarra,
    productos.nombre AS nombre,
    paises.nombre AS procedencia,
    IFNULL(proveedores.nombre,'-') AS proveedor,
    IFNULL(familias.nombre,'-') AS familia,
    rubros.nombre AS rubro,
    marcas.nombre AS marca,
    unidades.nombre AS medida,
    ubicaciones.nombre AS ubicacion,
    productos.costo AS costo,
    productos.incremento1 AS minorista,
    productos.incremento2 AS mayorista,
    productos.precio_maximo AS preciominorista,
    productos.precio_minimo AS preciomayorista,
    productos.ivaporcentaje AS iva,
    albumfotos_productos.foto,
    (CASE
		WHEN productos.tipo_producto=1 THEN 'Mercadería'
        WHEN productos.tipo_producto=2 THEN 'Materia Prima'
        WHEN productos.tipo_producto=3 THEN 'Producto en Proceso'
        WHEN productos.tipo_producto=4 THEN 'Producto Terminados'
        WHEN productos.tipo_producto=5 THEN 'Importación en Curso'
        WHEN productos.tipo_producto=6 THEN 'Servicios'
        ELSE ' '
	END) AS tipoproducto,
    productos.conversion AS conversion,
    IFNULL(productos.conteomayorista,0) AS cmayorista,
    productos.stockminimo AS stockminimo,
    (CASE WHEN productos.estado=1 THEN 'Activo' ELSE 'Inactivo' END) AS estado,
    IFNULL(productos.observacion,'SIN OBSERVACIONES') AS observaciones
FROM productos
LEFT JOIN Paises ON paises.codigo=productos.paises
LEFT JOIN rubros ON rubros.codigo=productos.rubro
LEFT JOIN marcas ON marcas.codigo=productos.marca
LEFT JOIN unidades ON unidades.codigo=productos.medida
LEFT JOIN ubicaciones ON ubicaciones.codigo=productos.ubicacion
LEFT JOIN proveedores ON proveedores.codigo=productos.proveedor
LEFT JOIN familias ON familias.codigo=productos.codfamilia
LEFT JOIN albumfotos_productos ON albumfotos_productos.codigo=productos.codigo
WHERE productos.codigo=$P{cCodigo}
ORDER BY productos.codigo]]>
	</queryString>
