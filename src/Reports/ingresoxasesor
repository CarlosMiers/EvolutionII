	<parameter name="FechaFinal" class="java.util.Date"/>
	<parameter name="nMoneda" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT numero,prestamos.fecha,moneda,sucursal,acreedor,socio,monto_entregar,
importe,prestamos.tipo,tasa,interes,ivainteres,totalprestamo,
clientes.nombre AS nombrecliente,comprobantes.nomalias,
monedas.etiqueta
FROM prestamos
INNER JOIN clientes
ON clientes.codigo=prestamos.socio
INNER JOIN comprobantes
ON comprobantes.codigo=prestamos.tipo
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
WHERE prestamos.fecha BETWEEN $P{FechaInicio} AND $P{FechaFinal}
AND prestamos.moneda=$P{nMoneda}
ORDER BY tipo,moneda,fecha]]>
	</queryString>
