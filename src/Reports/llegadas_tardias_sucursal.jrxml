	<queryString>
		<![CDATA[SELECT
     numero,
     concat(nombres,' ',apellidos) as nombrecompleto,
	idsucursal,
     cedula,
     fecha,
     horas,
     unidmed,
     importe,
     sucursales.nombre as sucursal
FROM
     llegadas_tardias
     INNER JOIN empleados ON llegadas_tardias.funcionario = empleados.codigo
     INNER JOIN sucursales ON empleados.sucursal = sucursales.codigo
WHERE
     llegadas_tardias.fecha >= $P{FechaI}
     and llegadas_tardias.fecha <= $P{FechaF}
and llegadas_tardias.idsucursal=$P{cSucursal}
ORDER BY idsucursal, empleados.codigo]]>
	</queryString>
