	<queryString>
		<![CDATA[SELECT numero,fecha,comercio,socio,plazo,usuarioalta,clientes.nombre AS nombresocio,
clientes.direccion,clientes.ruc,retenciones,
importe,monto_cuota,usuarios.last_name AS nombreusuario,casas.nombre AS nombrecomercio
FROM ordenes_de_credito
INNER JOIN clientes
ON clientes.codigo=ordenes_de_credito.socio
LEFT JOIN usuarios
ON usuarios.employee_id=ordenes_de_credito.usuarioalta
LEFT JOIN casas
ON casas.codigo=ordenes_de_credito.comercio
WHERE numero=$P{nNumero}]]>
	</queryString>
