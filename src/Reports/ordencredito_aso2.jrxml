	<queryString>
		<![CDATA[SELECT numero,fecha,comercio,socio,plazo,usuarioalta,clientes.nombre AS nombresocio,
clientes.direccion,clientes.ruc,
importe,monto_cuota,usuarios.last_name AS nombreusuario,casas.nombre AS nombrecomercio,
(SELECT nombre
FROM clientes
WHERE clientes.codigo=ordenes_de_credito.garante) nombregarante,
(SELECT direccion
FROM clientes
WHERE clientes.codigo=ordenes_de_credito.garante) direcciongarante,
(SELECT ruc
FROM clientes
WHERE clientes.codigo=ordenes_de_credito.garante) rucgarante
FROM ordenes_de_credito
INNER JOIN clientes
ON clientes.codigo=ordenes_de_credito.socio
LEFT JOIN usuarios
ON usuarios.employee_id=ordenes_de_credito.usuarioalta
LEFT JOIN casas
ON casas.codigo=ordenes_de_credito.comercio
WHERE numero=$P{nNumero}]]>
	</queryString>
