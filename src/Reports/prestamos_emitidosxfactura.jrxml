	<queryString>
		<![CDATA[SELECT numero,prestamos.fecha,moneda,sucursal,acreedor,socio,monto_entregar,oficial,
importe,prestamos.tipo,tasa,interes,ivainteres,totalprestamo,monto_cuota,
clientes.nombre AS nombrecliente,comprobantes.nomalias,gastos_escritura,segurovida,deducciones,
comision_deudor,monedas.etiqueta,
detalle_forma_pago.forma,detalle_forma_pago.banco,detalle_forma_pago.nrocheque,
formaspago.nombre AS nombreformapago,bancos.nombre AS nombrebanco,
vendedores.nombre AS nombreasesor,detalle_forma_pago.netocobrado,
(SELECT SUM(ivaxinteres)
FROM detalle_prestamo WHERE numero=nprestamo) totalivainteres,
(SELECT factura
FROM cabecera_ventas WHERE creferencia=idprestamos) nrofactura
FROM prestamos
INNER JOIN clientes
ON clientes.codigo=prestamos.socio
INNER JOIN comprobantes
ON comprobantes.codigo=prestamos.tipo
INNER JOIN monedas
ON monedas.codigo=prestamos.moneda
LEFT JOIN detalle_forma_pago
ON detalle_forma_pago.idmovimiento = prestamos.idprestamos
LEFT JOIN formaspago
ON formaspago.codigo=detalle_forma_pago.forma
LEFT JOIN bancos
ON bancos.codigo=detalle_forma_pago.banco
LEFT JOIN vendedores
ON vendedores.codigo=prestamos.oficial
WHERE prestamos.fecha BETWEEN $P{FechaI} AND $P{FechaF}
and prestamos.estado="DESEMBOLSADO"
ORDER BY tipo,moneda,numero]]>
	</queryString>
