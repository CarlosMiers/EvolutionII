	<parameter name="cCliente" class="java.lang.String"/>
	<parameter name="FechaI" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT YEAR(vencimiento)AS anio,MONTH(vencimiento) AS mes,SUM(saldo) AS acobrar,
CASE MONTH(vencimiento)
WHEN 1 THEN 'ENE'
WHEN 2 THEN 'FEB'
WHEN 3 THEN 'MAR'
WHEN 4 THEN 'ABR'
WHEN 5 THEN 'MAY'
WHEN 6 THEN 'JUN'
WHEN 7 THEN 'JUL'
WHEN 8 THEN 'AGO'
WHEN 9 THEN 'SET'
WHEN 10 THEN 'OCT'
WHEN 11 THEN 'NOV'
WHEN 12 THEN 'DIC'
END AS nombremes
FROM cuenta_clientes
WHERE vencimiento>DATE_SUB($P{FechaI}, INTERVAL day($P{FechaI}) DAY)
AND cliente=$P{cCliente}
AND cuenta_clientes.saldo>0
GROUP BY anio,mes
LIMIT 6]]>
	</queryString>
