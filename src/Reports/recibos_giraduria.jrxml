	<queryString>
		<![CDATA[SELECT cierre_cobranzas.fecha,cierre_cobranzas.documento,cierre_cobranzas.cliente,cierre_cobranzas.cobrado,clientes.nombre,clientes.ruc,
cierre_cobranzas.cuota,cierre_cobranzas.numerocuota,cierre_cobranzas.amortiza,cierre_cobranzas.minteres,
0.0 as gastos, 0.0 as mora, 0.0 as punitorio
FROM cierre_cobranzas
INNER JOIN clientes
ON clientes.codigo=cierre_cobranzas.cliente
WHERE cierre_cobranzas.cliente=$P{cCliente}
 AND cierre_cobranzas.giraduria=$P{nGiraduria}
AND cierre_cobranzas.fecha_cobro between $P{FechaI}
AND $P{FechaF}
ORDER BY cierre_cobranzas.cliente]]>
	</queryString>
