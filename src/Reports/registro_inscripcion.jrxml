	<queryString>
		<![CDATA[SELECT consultas_servicios.numero,consultas_servicios.fecha,consultas_servicios.vencimiento,consultas_servicios.periodopago,
CASE consultas_servicios.periodopago
  WHEN 1 THEN 'DIARIO'
  WHEN 2 THEN 'SEMANAL'
  WHEN 3 THEN 'QUINCENAL'
  WHEN 4 THEN 'MENSUAL'
 END AS nombremes,
 consultas_servicios.cliente,consultas_servicios.totales,clientes.fechanacimiento,
 clientes.nombre AS nombrecliente,clientes.ruc,clientes.telefono,
 productos.nombre AS nombreservicio,detalle_consulta_servicios.importe
FROM consultas_servicios
INNER JOIN clientes
ON clientes.codigo=consultas_servicios.cliente
INNER JOIN detalle_consulta_servicios
ON detalle_consulta_servicios.dnumero=consultas_servicios.numero
INNER JOIN productos
ON productos.codigo=detalle_consulta_servicios.servicio
WHERE consultas_servicios.numero=$P{cNumero}
ORDER BY consultas_servicios.numero]]>
	</queryString>
