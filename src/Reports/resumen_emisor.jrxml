	<parameter name="FechaInicio" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT cartera_clientes.moneda,monedas.nombre,
emisores.nombre AS nombreemisor,emisores.nomalias,
SUM(valor_inversion) AS totalinversion
FROM cartera_clientes
LEFT JOIN monedas
ON cartera_clientes.moneda=monedas.codigo
LEFT JOIN emisores
ON cartera_clientes.emisor=emisores.codigo
WHERE cartera_clientes.comprador=$P{cEmisor}
 AND IF($P{cMoneda}<>0,cartera_clientes.moneda=$P{cMoneda},TRUE)
AND cartera_clientes.fechacierre<=$P{FechaInicio}
 AND operacion=1
AND cartera_clientes.estado=1
GROUP BY emisores.codigo,monedas.codigo
ORDER BY monedas.codigo,emisores.codigo]]>
	</queryString>
