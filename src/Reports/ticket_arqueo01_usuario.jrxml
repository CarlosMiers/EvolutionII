	<queryString>
		<![CDATA[SELECT SUM(IF(forma = 1, netocobrado, 0)) efectivo,
SUM(IF(cabecera_ventas.comprobante=3 AND cabecera_ventas.cliente=1, cabecera_ventas.totalneto, 0)) devoluciones,
SUM(IF(forma = 2, netocobrado, 0)) cheque,
SUM(IF(forma = 3, netocobrado, 0)) tarjeta,
SUM(netocobrado) as totalcobro
FROM cabecera_ventas
LEFT JOIN detalle_forma_cobro
ON cabecera_ventas.creferencia=detalle_forma_cobro.idmovimiento
WHERE idusuario=$P{nusuario}
AND fecha=$P{dFecha}]]>
	</queryString>
