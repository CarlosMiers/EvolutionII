	<queryString>
		<![CDATA[SELECT cuenta_clientes.*,clientes.categoria,clientes.asesor,clientes.nombre as nombrecliente,vendedores.nombre as nombreasesor,DATEDIFF(curdate(),cuenta_clientes.vencimiento) AS atraso
FROM cuenta_clientes
INNER JOIN CLIENTES
ON clientes.codigo=cuenta_clientes.cliente
INNER JOIN vendedores
ON vendedores.codigo=clientes.asesor
WHERE  clientes.categoria NOT IN (23,24)
and cuenta_clientes.comprobante=$P{tipooperacion}
and cuenta_clientes.saldo>0
and cuenta_clientes.moneda=$P{nMoneda}
and cuenta_clientes.vencimiento BETWEEN  $P{FechaInicio} AND $P{FechaFinal}
ORDER by cuenta_clientes.vencimiento]]>
	</queryString>
